{
  "name": "v-region",
  "description": "A simple region selector, provide Chinese administrative division data",
  "version": "2.2.1",
  "author": "TerryZ <terry5@foxmail.com>",
  "license": "MIT",
  "main": "dist/v-region.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test:unit": "mocha-webpack --webpack-config webpack.config.js --require tests/unit/setup.js tests/unit/**/*.spec.js",
    "cover": "cross-env NODE_ENV=coverage nyc --reporter=lcov --reporter=text npm run test:unit"
  },
  "dependencies": {
    "v-dropdown": "2.1.1"
  },
  "keywords": [
    "front-end",
    "javascript",
    "web",
    "vue",
    "vuejs",
    "region",
    "city",
    "town",
    "area",
    "selector"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.2.0",
    "cross-env": "^5.0.5",
    "css-loader": "^3.2.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.2.1",
    "mocha-webpack": "^1.1.0",
    "node-sass": "^4.12.0",
    "nyc": "^14.1.1",
    "sass-loader": "^6.0.6",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "url-loader": "^2.2.0",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-node-externals": "^1.7.2"
  },
  "nyc": {
    "include": [
      "src/components/CityPicker.js",
      "src/components/ColumnGroup.js",
      "src/components/Group.js",
      "src/components/Text.js",
      "src/components/SelectGroup.js"
    ],
    "instrument": false,
    "sourceMap": false
  }
}
